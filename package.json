    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@clack/prompts": "^0.7.0",
    "@supabase/supabase-js": "^2.46.2",
    "@tensorflow/tfjs": "^4.22.0",
    "ai": "^3.1.0",
    "chalk": "^5.3.0",
    "cli-cursor": "^5.0.0",
    "cli-spinners": "^3.2.0",
    "clipboardy": "^4.0.0",
